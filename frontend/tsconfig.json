{
  "compilerOptions": {
      "target": "es2016",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
     "jsx": "react-jsx",
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "include": [
   "src/**/*.ts","**/*.ts", "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}
